MSYS_NO_PATHCONV=1 docker run -it --rm --name gemini \
  -v "C:/Users/enisb/.gemini:/home/node/.gemini" \
  -e GOOGLE_CLOUD_PROJECT=gen-lang-client-0839142475 \
  weaponsforge/gemini-cli sh -lc 'gemini --debug'
  
Step 2: 
Click link and long in from step 1

Step 3: 
# Paste URL
docker exec gemini sh -lc 'node -e "const http=require(\"http\");const u=process.argv[1];http.get(u,r=>r.resume()).on(\"error\",e=>{console.error(e.message);process.exit(1);});" "<url>"'


Step 4; 
MSYS_NO_PATHCONV=1 docker run --rm -it \
    -v "$(echo ~)/.gemini":/home/node/.gemini \
    -v "$(pwd)":/work \
    -w /work \
